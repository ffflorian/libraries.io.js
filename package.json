{
  "author": "Florian Keller <github@floriankeller.de>",
  "dependencies": {
    "@types/node": "10.12.7",
    "axios": "0.18.0"
  },
  "description": "A libraries.io API client",
  "devDependencies": {
    "@types/jasmine": "3.3.0",
    "jasmine": "3.3.0",
    "nock": "10.0.3",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "touch": "3.1.0",
    "ts-node": "7.0.1",
    "typedoc": "0.13.0",
    "typescript": "3.2.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "api",
    "libraries.io",
    "libraries",
    "librariesio"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "libraries.io",
  "repository": "https://github.com/ffflorian/libraries.io.js.git",
  "scripts": {
    "build": "tsc",
    "build:docs": "typedoc --out docs/ --mode modules --excludePrivate --excludeExternals --exclude \"**/*(index|Endpoints|RequestService).ts\"",
    "clear": "rimraf dist",
    "clear:docs": "rimraf docs",
    "dist": "yarn clear && yarn build",
    "dist:docs": "yarn clear:docs && yarn build:docs && nodetouch docs/.nojekyll",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.ts\"",
    "test": "ts-node -P tsconfig.jasmine.json node_modules/.bin/jasmine"
  },
  "types": "dist/index.d.ts",
  "version": "1.2.1"
}
